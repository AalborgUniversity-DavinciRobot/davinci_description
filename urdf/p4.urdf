<?xml version="1.0"?>
<robot name="p4">
    <link name="tower">
        <visual>
            <origin xyz="0.0 0.0 1.00"/>
            <geometry>
                <box size="0.15 0.15 2.00" />
            </geometry>
            <material name="surgical gray">
                <color rgba="0.67 0.65 0.64 1.0" />
            </material>
        </visual>
    </link>

    <link name="p4-arm_base">
        <visual>
            <origin xyz="0.074 0.0 0.075" />
            <geometry>
                <cylinder radius="0.074" length="0.150" />
            </geometry>
            <material name="surgical gray" />
        </visual>
    </link>

    <link name="p4-arm1">
        <visual>
            <geometry>
                <mesh filename="package://davinci_description/meshes/p4-arm1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="surgical gray" />
        </visual>
    </link>

    <link name="p4-arm2">
        <visual>
            <geometry>
                <mesh filename="package://davinci_description/meshes/p4-arm2.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="surgical gray" />
        </visual>
    </link>

    <link name="p4-arm3">
        <visual>
            <geometry>
                <mesh filename="package://davinci_description/meshes/p4-arm3.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="surgical gray" />
        </visual>
    </link>

    <link name="p4-arm4">
        <visual>
            <geometry>
                <cylinder radius="0.029" length="0.3947" />
            </geometry>
            <origin xyz="0.19735 0.0 -0.14695" rpy="0 1.57080 0"/>
            <material name="surgical gray" />
        </visual>
    </link>

    <link name="p4-hand1">
        <visual>
            <geometry>
                <mesh filename="package://davinci_description/meshes/hand1.stl" scale="0.001 0.001 0.001"/>
                <!-- <box size="0.375 0.095 0.155" /> -->
            </geometry>
            <!-- <origin xyz="-0.08 0.0 0.08625"/> -->
            <material name="slate">
                <color rgba="0.2 0.2 0.2 1"/>
           </material>
        </visual>
    </link>

    <link name="p4-hand2">
        <visual>
            <geometry>
                <mesh filename="package://davinci_description/meshes/hand2.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="slate" />
        </visual>
    </link>

    <link name="p4-hand3">
        <visual>
            <geometry>
                <box size="0.035 0.08 0.42" />
            </geometry>
            <origin xyz="-0.0175 0.0 0.286" />
            <material name="slate" />
        </visual>
    </link>

    <link name="instrument_head">
        <visual>
            <geometry>
                <mesh filename="package://davinci_description/meshes/inst.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="blue">
                    <color rgba="0.3 0.3 1 1"/>
           </material>
        </visual>
    </link>

    <link name="instrument_spear">
        <visual>
            <geometry>
                <cylinder radius="0.004" length="0.4" />
            </geometry>
            <origin xyz="0.2 0 0" rpy="0 1.57075 0"/>
            <material name="slate" />
        </visual>
    </link>

    <link name="instrument_palm">
        <visual>
            <geometry>
                <cylinder radius="0.003" length="0.01" />
            </geometry>
            <origin xyz="0.005 0 0" rpy="0 1.57075 0" />
            <material name="gray">
                <color rgba="0.5 0.5 0.5 1"/>
            </material>
        </visual>
    </link>

    <link name="index_finger">
        <visual>
            <geometry>
                <cylinder radius="0.001" length="0.01" />
            </geometry>
            <origin xyz="0.005 -0.001 0" rpy="0 1.57075 0" />
            <material name="gray" />
        </visual>
    </link>

    <link name="thumb">
        <visual>
            <geometry>
                <cylinder radius="0.001" length="0.01" />
            </geometry>
            <origin xyz="0.005 0.001 0" rpy="0 1.57075 0" />
            <material name="gray" />
        </visual>
    </link>
     

    <joint name="p4-arm3_slide" type="prismatic">
        <parent link="tower" />
        <child link="p4-arm_base" />
        <axis xyz="0 0 1" />
        <origin xyz="0.075 0 0" />
        <limit lower="0.5" upper="1.0" effort="10" velocity="1" />
    </joint>

    <joint name="p4-yaw1" type="revolute">
        <parent link="p4-arm_base" />
        <child link="p4-arm1" />
        <origin xyz="0.074 0.0 0.150" />
        <axis xyz="0 0 1"/>
        <limit lower="-1.57080" upper="1.57080" effort="10" velocity="1" />
    </joint>

    <joint name="p4-yaw2" type="revolute">
        <parent link="p4-arm1" />
        <child link="p4-arm2" />
        <origin xyz="0.58285 0.0 -0.015" />
        <axis xyz="0 0 1"/>
        <limit lower="-1.57080" upper="1.57080" effort="10" velocity="1" />
    </joint>

    <joint name="p4-yaw3" type="revolute">
        <parent link="p4-arm2" />
        <child link="p4-arm3" />
        <origin xyz="0.47985 0.0 0.0" />
        <axis xyz="0 0 1"/>
        <limit lower="-1.57080" upper="1.57080" effort="10" velocity="1" />
    </joint>

    <joint name="p4-roll1" type="revolute">
        <parent link="p4-arm3" />
        <child link="p4-arm4" />
        <origin xyz="0.05775 0.0 0.19725" />
        <axis xyz="1 0 0"/>
        <limit lower="-1.57080" upper="1.57080" effort="10" velocity="1" />
    </joint>

    <joint name="p4-yaw4" type="revolute">
        <parent link="p4-arm4" />
        <child link="p4-hand1" />
        <origin xyz="0.3495 0.0 -0.11045" />
        <axis xyz="0 0 1"/>
        <limit lower="-1.57080" upper="1.57080" effort="10" velocity="1" />
    </joint>

    <joint name="p4-roll2" type="revolute">
        <parent link="p4-hand1" />
        <child link="p4-hand2" />
        <origin xyz="0.1075 0.0 0.14775" />
        <axis xyz="1 0 0"/>
        <limit lower="-1.57080" upper="1.57080" effort="10" velocity="1" />
    </joint>

    <joint name="p4-pitch1" type="revolute">
        <parent link="p4-hand2" />
        <child link="p4-hand3" />
        <origin xyz="0.363 0.0 0.0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-0.78540" upper="0" effort="10" velocity="1" />
    </joint>

    <joint name="instrument_slide" type="prismatic">
        <parent link="p4-hand3" />
        <child link="instrument_head" />
        <axis xyz="1 0 0" />
        <origin xyz="0.0 0.0 0.45" rpy="0.0 1.57080 0.0"/>
        <limit lower="0" upper="0.2" effort="10" velocity="1" />
    </joint>

    <joint name="spear_swivel" type="revolute">
        <parent link="instrument_head" />
        <child link="instrument_spear" />
        <origin xyz="0.07 0 0.008" />
        <axis xyz="1 0 0" />
        <limit lower="-3.14159" upper="3.14159" effort="10" velocity="1" />
    </joint>

    <joint name="wrist" type="revolute">
        <parent link="instrument_spear" />
        <child link="instrument_palm" />
        <axis xyz="0 1 0" />
        <origin xyz="0.4 0 0" />
        <limit lower="-0.26180" upper="0.26180" effort="10" velocity="1" />
    </joint>

    <joint name="jaw" type="revolute">
        <parent link="index_finger" />
        <child link="thumb" />
        <axis xyz="0 0 1" />
        <limit lower="0" upper="0.52360" effort="10" velocity="1" />
    </joint>

    <joint name="index_joint" type="revolute">
        <parent link="instrument_palm" />
        <child link="index_finger" />
        <axis xyz="0 0 1" />
        <origin xyz="0.01 0 0" />
        <limit lower="-0.26180" upper="0.26180" effort="10" velocity="1" />
    </joint>

</robot>
